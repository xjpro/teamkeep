<!DOCTYPE html>
<html ng-app="teamkeep">
    <head>
        <title>@ViewBag.Title | Teamkeep</title>
        @Html.Partial("Includes")
        <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.0.7/angular.min.js"></script>
        <script src="/Scripts/app/teamkeep.js"></script>
        <script src="/Scripts/app/shared/User.js"></script>
        <script src="/Scripts/app/tutorial/TutorialController.js"></script>
    </head>
    <body>
        <script>
            var viewData = @Html.Raw(Json.Encode(Model));
        </script>
        
        <div id="header">
            @Html.Partial("Header")    
        </div>
        @if (ViewBag.TeamHeader != null && ViewBag.TeamHeader)
        {
            @Html.Partial("~/Views/Team/TeamHeader.cshtml")
        }
        
        @RenderBody()   
        @Html.Partial("Footer")
        @Html.Partial("Alerts")
        
    </body>
</html>
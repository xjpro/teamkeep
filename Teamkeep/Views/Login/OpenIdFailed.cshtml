@model Teamkeep.Models.OpenAuthFail 
@{
    ViewBag.App = "teamkeep-public";
    ViewBag.Title = "Free Sports Team Management Software";
    ViewBag.UseHeaderFooter = true;
    Layout = "~/Views/Shared/Master.cshtml";
}

<div class="buffered container">
    <h1>Google Authentication Failed</h1>
    <p>
        We're very sorry, it appears Google authentication failed.
    </p>
    <p>
        We recommend you clear your browser cache or cookies and try again.
    </p>

    <div class="hide">
        <div class="form-group">
            <label>Status</label>
            <p>@Model.Status</p>
        </div>

        <div class="form-group">
            <label>Exception</label>
            <p>@Model.Exception</p>
        </div>
    </div>
</div>